
package while_ile_faktöriyel_bulma;

import java.util.Scanner;


public class While_ile_faktöriyel_bulma {

    public static void main(String[] args) {
        
        int i=1;
        Scanner input=new Scanner(System.in);
        int sayi=input.nextInt();
        int fakt=1;
        
        while(i<=sayi){
            
            fakt*=i;
            i++;
        }
        System.out.println("Sayının faktöriyel değeri :"+fakt);
        
    }
    
}
