package not_uygulaması;

import java.util.Scanner;

/**
 *
 * @author Emre YILDIZ
 */
public class Not_uygulaması {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        
        System.out.println("Lütfen not giriniz !");
        Scanner s = new Scanner (System.in);
        int i=s.nextInt();
        
        if (85<=i && i<=100) {
            System.out.println("Çok iyi not");
        }
        
        else if (70<=i && i<=84) {
            System.out.println("idare eder not");
        }
        
        else if (55<=i && i<=69) {
            System.out.println("hadi yine yırttın");
        }
        
        else{
            System.out.println("sıçtın dostum,kaldın...");
        }
        
    }
    
}
