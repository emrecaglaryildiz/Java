
package contunie_örnek;


public class Contunie_örnek {

    public static void main(String[] args) {
               
        int i=5;
        while (i<12) {
            if (i==8) {
                i++;
                continue;
            }
        System.out.println("i= "+i);
        i++;
            
        }
        
        
        
    }
    
}
