package fibonacci_diziler_örnek;

/**
 *
 * @author Emre YILDIZ
 */
public class Fibonacci_diziler_örnek {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // Fibonacci Diziler
        int fib[] = new int [10];
        fib[0]=1;
        fib[1]=1;
        for (int i = 0; i < 8; i++) {
            fib[i+2]=fib[i+1]+fib[i];
        }
        for (int i = 0; i < 10; i++) {
            System.out.println(fib[i]);
        }
    }
    
}
